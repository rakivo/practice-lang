var sum 0 end
var is_mod3 0 end
var is_mod5 0 end

proc solve do
  0 while dup 999 < do
    # x += 1
    1 +

    # x % 3 == 0
    dup 3 % 0 = !is_mod3

    # x % 5 == 0
    dup 5 % 0 = !is_mod5

    # is_mod3 | is_mod5
    is_mod3! is_mod5! | if
      # sum += x
      dup sum! + !sum
    end
  end
end

func main int do
  solve!
  sum! . 0
end
